<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import entities.MapView?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.MapPageController" >
    <HBox alignment="CENTER" spacing="20" AnchorPane.rightAnchor="0" AnchorPane.leftAnchor="0">
        <Button fx:id="toMap1Btn" prefWidth="70.0" text="Map 1" onAction="#toMap1" />
        <Button fx:id="toMap2Btn" prefWidth="70.0" text="Map 2" onAction="#toMap2" />
        <Button fx:id="toMap3Btn" prefWidth="70.0" text="Map 3" onAction="#toMap3" />
    </HBox>
    <MapView fx:id="mapView" AnchorPane.rightAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="30" AnchorPane.bottomAnchor="0"></MapView>
</AnchorPane>
